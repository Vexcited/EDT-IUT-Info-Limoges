{
  "name": "edt-iut-info-limoges",
  "version": "1.6.10",
  "description": "Emploi du temps de l'IUT Informatique de Limoges",
  "license": "MIT",
  "private": false,
  "files": [
    "dist"
  ],
  "keywords": [
    "timetable",
    "university",
    "pdf",
    "iut",
    "France"
  ],
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/index.mjs"
    }
  },
  "scripts": {
    "release": "release-it",
    "build": "tsup ./src/index.ts --clean --dts --minify terser --format esm,cjs --sourcemap",
    "test": "bun test"
  },
  "release-it": {
    "hooks": {
      "before:init": [
        "pnpm build"
      ]
    },
    "git": {
      "commitMessage": "chore: release v${version}",
      "tagAnnotation": "Release v${version}",
      "tagName": "v${version}"
    },
    "github": {
      "release": false
    },
    "npm": {
      "publish": true,
      "skipChecks": true,
      "versionArgs": [
        "--workspaces-update=false"
      ]
    }
  },
  "devDependencies": {
    "@types/bun": "^1.1.14",
    "@types/jest": "^29.5.12",
    "@types/luxon": "^3.4.2",
    "@types/node": "^20.11.24",
    "release-it": "^17.1.1",
    "terser": "^5.28.1",
    "tsup": "^7.2.0"
  },
  "author": "Mikkel RINGAUD <mikkel@milescode.dev> (https://github.com/Vexcited)",
  "repository": "github:Vexcited/EDT-IUT-Info-Limoges",
  "dependencies": {
    "@literate.ink/pdf-inspector": "^1.0.0",
    "luxon": "^3.4.4"
  }
}
