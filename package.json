{
  "name": "edt-iut-info-limoges-monorepo",
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "library:release": "pnpm --filter edt-iut-info-limoges run release",
    "library:test": "pnpm --filter edt-iut-info-limoges run test",
    "library:build": "pnpm --filter edt-iut-info-limoges run build",
    "website:dev": "pnpm --filter edt-iut-info-limoges-website run dev",
    "website:build": "pnpm --filter edt-iut-info-limoges-website run build",
    "full:build": "pnpm run library:build && pnpm run website:build"
  },
  "private": true,
  "license": "MIT",
  "author": "Mikkel ALMONTE--RINGAUD <contact@vexcited.com> (https://github.com/Vexcited)",
  "repository": "github:Vexcited/EDT-IUT-Info-Limoges"
}
