{
  "name": "edt-iut-info-limoges-monorepo",
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "library:release": "bun run --filter edt-iut-info-limoges release",
    "library:test": "bun run --filter edt-iut-info-limoges test",
    "library:build": "bun run --filter edt-iut-info-limoges build",
    "website:dev": "bun run --filter edt-iut-info-limoges-website dev",
    "website:build": "bun run --filter edt-iut-info-limoges-website build",
    "full:build": "bun run library:build && bun run website:build"
  },
  "private": true,
  "license": "MIT",
  "author": "Mikkel ALMONTE--RINGAUD <contact@vexcited.com> (https://github.com/Vexcited)",
  "repository": "github:Vexcited/EDT-IUT-Info-Limoges"
}
